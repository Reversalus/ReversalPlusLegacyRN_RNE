{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": [
      "esnext",
      "dom"
    ],
    "jsx": "react",
    "strict": true,
    "noEmit": true,                          // Set noEmit to true
    "allowImportingTsExtensions": true,      // This can now be used safely
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "babel.config.js", "metro.config.js", "webpack.config.js"]
}
